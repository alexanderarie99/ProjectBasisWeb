<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>Home</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>Mahasiswa</servlet-name>
        <servlet-class>Mahasiswa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin</servlet-name>
        <servlet-class>Admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KartuKRS</servlet-name>
        <servlet-class>KartuKRS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TanpaLogin</servlet-name>
        <servlet-class>TanpaLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>KartuKRS</servlet-name>
        <url-pattern>/KartuKRS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TanpaLogin</servlet-name>
        <url-pattern>/Redirect</url-pattern>
        <url-pattern>/TanpaLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Mahasiswa</servlet-name>
        <url-pattern>/Mahasiswa</url-pattern>
        <url-pattern>/mahasiswa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin</servlet-name>
        <url-pattern>/Admin</url-pattern>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/Home</url-pattern>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>TanpaLogin.java</welcome-file>
    </welcome-file-list>
</web-app>
